struct users {
    char *username;     /*username*/
    char *nome;         /*Nome da pessoa*/
    char *genero;       /* genero da pessoa*/
    char *datAniv;      /*data de nascimento*/
    char *accDate;      /*data de criação da conta*/
    char *payMethod;    /* método de pagamento*/
    char *accStatusU;    /* estado da conta*/
} Users;

struct drivers {

}



int isvalid (Users u) {
    if (u->accStatusU=="active")
        return 1;
    else
        return 0;
}
